    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/finetune.py",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "args": [
                "agent=diayn",
                "task=walker_flip",
                "snapshot_ts=100000",
                "obs_type=states",
                "reward_free=false",
                "use_wandb=True"
            ]
        }
    ]
}